<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - GeradorDeProtocolo</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>

    <header id="hea">
        <nav id="nav"  class="navbar navbar-expand-sm navbar-toggleable-sm  border-bottom box-shadow mb-3" >
            <div class="container">
                <a class="nav-link" id="a"  asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <ul class="navbar-nav flex-grow-1">

                        <li class="nav-item">
                            <a class="nav-link" id="a" asp-area="" asp-controller="Assunto" asp-action="Index">Assunto</a>
                        </li>
                        <li class="nav-item">
                            <a   class="nav-link" id="a" asp-area="" asp-controller="GrupoAssunto" asp-action="Index">Grupo de Assunto</a>
                        </li>
                        <li class="nav-item">
                            <a   class="nav-link" id="a"  asp-area="" asp-controller="Local" asp-action="Index">Local</a>
                        </li>
                        <li class="nav-item">
                            <a  class="nav-link"  id="a"   asp-area="" asp-controller="Sublocal" asp-action="Index">SubLocal </a>
                        </li>
                        <li class="nav-item">
                            <a  class="nav-link"  id="a" asp-area="" asp-controller="OrigemDeAbertura" asp-action="Index">Origem de Abertura </a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - GeradorDeProtocolo - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
